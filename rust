use std::{thread, time};

fn main() {
    println!("Machine Started...");
    
    println!("Conveyor Running...");
    thread::sleep(time::Duration::from_secs(2));
    
    println!("Cutter Activated...");
    thread::sleep(time::Duration::from_secs(2));
    
    println!("Cutting Complete.");
    println!("Machine Stopped.");
}
